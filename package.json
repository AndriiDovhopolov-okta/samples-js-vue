    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-vue": "^4.2.0",
    "find-process": "^1.1.0",
    "forever-monitor": "^1.7.1",
    "jasmine-reporters": "^2.2.0",
    "platform": "^1.3.5",
    "protractor": "^5.1.0",
    "wait-on": "^2.0.2"
  },
  "bin": {},
  "private": true
}
